<!DOCTYPE html>
<html>
  <head>
    <title>RuPaul for Senate 2020</title>
    <%= csrf_meta_tags %>
    <!-- Bootstrap for defaults -->
    <%= stylesheet_link_tag    'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css'%>
    <!-- Google Font -->
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Open+Sans' %>
    <!-- Custrom CSS -->
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

  </head>

  <body>
    <img src="/../assets/rupaul_senate.png" width="100%">
    <%= render 'layouts/navbar' %>

    <div class="dark_background" class="container">
        <%= notice %>
        <%= yield %>
    </div>
  </body>
</html>
